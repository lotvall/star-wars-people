{
  "name": "star-wars-people",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "server": "nodemon --exec babel-node index.js",
    "start": "node ./build/index.js",
    "build": "npm run clean && npm run build-server && npm run build-schema && npm run build-resolvers && npm run build-helpers",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./index.js -s",
    "build-schema": "babel -d ./build/schema ./schema -s",
    "build-resolvers": "babel -d ./build/resolvers ./resolvers -s",
    "build-helpers": "babel -d ./build/helpers ./helpers -s"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "http": "0.0.0",
    "merge-graphql-schemas": "^1.5.8",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "nodemon": "^1.18.10"
  }
}
